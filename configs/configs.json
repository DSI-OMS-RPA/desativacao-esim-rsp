{
    "process": {
        "name": "Desativação de Cartões eSIM - RSP",
        "identifier": "AGT009DSI",
        "description": "Processo automatizado de desativação de cartões eSIM na plataforma RSP (Remote SIM Provisioning), com base em dados de desativação provenientes do NGIN/Siebel.",
        "version": "1.2.1",
        "execMode": "daily",
        "execTime": "08:30",
        "timezone": "Atlantic/Cape_Verde"
    },
    "retry_policy": {
        "max_retries": 3,
        "delay_seconds": 5,
        "backoff_factor": 2
    },
    "env_path": "configs/.env",
    "esim_range": {
        "start": 8923801000101000000,
        "end": 8923801000101999999
    },
    "report": {
        "from_mail": "dsi-suporte-rpa@cvt.cv",
        "to": "joselito.coutinho@cvt.cv",
        "cc": [
            "luis.varela@cvt.cv",
            "alertas.rpa@cvt.cv"
        ],
        "subject": "Desativação de Cartões eSIM - RSP"
    },
    "error_report": {
        "from_mail": "dsi-suporte-rpa@cvt.cv",
        "to": "alertas.rpa@cvt.cv",
        "subject": "Erro: Desativação de Cartões eSIM - RSP"
    },
    "no_files_alert": {
        "recipients": ["dsi-support-crm-aia-oss@cvt.cv"]
    },
    "database": {
        "webmail": {
            "table": "platforms",
            "where": {
                "clause": "identifier = %s",
                "params": "CVT_Webmail"
            }
        }
    }
}
